num1=int(input('coloque o primeiro numero: '))
num2=int(input('coloque o segundo numero : '))

if num1 < num2 :
    print('o numero {} é maior que o {}'.format(num2,num1))
elif num1 > num2:
    print('o numero {} é maior que o numero {}'.format(num1,num2))

else:
    print('não existe numero maior ')
